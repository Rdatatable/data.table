msgstr "排序必须是整数向量"

#: reorder.c:34
#, c-format
msgid "nrow(x)[%d]!=length(order)[%d]"
msgstr "nrow(x)[%d] 不等于 length(order)[%d]"

#: reorder.c:48
#, c-format
msgid "order is not a permutation of 1:nrow[%d]"
msgstr "顺序与 1 到 nrow[%d] 的排列不同"

#: reorder.c:57
#, c-format
