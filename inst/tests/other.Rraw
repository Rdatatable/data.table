if (loaded[["yaml"]]) {  # csvy; #1701. Was 2032-2033 in tests.Rraw, #5516
  f = testDir("csvy/test.csvy")
  DT = data.table(var1 = c("A", "B"),
                  var2 = c(1L, 3L),
                  var3 = c(2.5, 4.3))
