  setattr(DT_yaml, 'yaml_metadata',
    list(name = "my-dataset",
         source = "https://github.com/leeper/csvy/tree/master/inst/examples",
         schema = list(fields = list(
           list(name = "var1", title = "variable 1", type = "string",
